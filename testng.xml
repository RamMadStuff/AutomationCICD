<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel = "tests" name="Suite">

  <listeners>
    <listener class-name="SeliniumMaven.TestComponents.Listeners"/>
  </listeners>

  <test name="Submit Order Test">
    <classes>
      <class name="SeliniumMaven.Tests.SubmitOrderTest"/>
    </classes>
  </test>

  <test thread-count="5" name="Error Validation Test">
    <classes>
      <class name="SeliniumMaven.Tests.ErrorValidationsTest"/>
    </classes>
  </test>

</suite>
